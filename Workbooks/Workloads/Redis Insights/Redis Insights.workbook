            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "InsightsMetrics\r\n| where (Computer in ({OnboardedComputers}) or '*' in ({OnboardedComputers}))\r\n  and Name == \"evicted_keys\"\r\n| order by Computer asc, TimeGenerated asc\r\n| extend Value = iff(Computer == prev(Computer) and Val > prev(Val), Val - prev(Val), real(0))\r\n| make-series evicted_keys = sum(Value) default = 0 on TimeGenerated from {TimeRange:start} to {TimeRange:end} step {TimeRange:grain} by Computer",
              "size": 0,
              "aggregation": 5,
              "showAnalytics": true,
              "title": "Evicted keys",
              "noDataMessage": "No available data",
              "timeContext": {
                "durationMs": 14400000
              },
              "timeContextFromParameter": "TimeRange",
              "showExportToExcel": true,
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [
