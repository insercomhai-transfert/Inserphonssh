    {
      "type": 1,
      "content": {
        "json": "### Collection logs from {VirtualMachine:name}"
      },
      "name": "headertext"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "union telegraflogs_CL, waagentlogs_CL\n| order by TimeGenerated desc\n| extend Message = iff(Type == 'telegraflogs_CL', iff('{LogsFilter}' == \"All Logs\", extract(@'.+Z D! \\[outputs.socket_writer\\] (.+)', 1, RawData), \nextract(@'.+Z E! (.+)', 1, RawData)), iff('{LogsFilter}' == \"All Logs\", extract(@'.+Z (.+)', 1, RawData), extract(@'.+Z (.+(Microsoft\\.Azure\\.Monitor\\.WorkloadInsights\\.Test\\.Workload\\.LinuxConfigAgent|Microsoft\\.Azure\\.Monitor\\.WorkloadInsights\\.Test\\.Workload\\.LinuxInstallerAgent)(.+))', 1, RawData) ) )\n| where ('{LogsFilter}' == \"Debug Logs\" and Message != \"\") or ('{LogsFilter}' == \"All Logs\")\n| project Time = TimeGenerated, Message = iff(Message == '', RawData, Message), ['Log type'] = iff(Type == 'telegraflogs_CL', 'Telegraf', iff('{LogsFilter}' == \"Debug Logs\", \"Telegraf setup\", 'WA Agent'))\n| take 501",
        "size": 3,
        "showAnalytics": true,
        "timeContext": {
          "durationMs": 0
        },
        "timeContextFromParameter": "TimeRange",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.compute/virtualmachines",
        "crossComponentResources": [
          "{VirtualMachine}"
        ],
        "gridSettings": {
          "formatters": [
            {
              "columnMatch": "Time",
              "formatter": 6,
              "formatOptions": {},
              "numberFormat": {
                "unit": 0,
                "options": {
                  "style": "decimal",
                  "useGrouping": false
                }
              },
              "dateFormat": {
                "showUtcTime": null,
                "formatName": "shortDateTimePattern"
              }
            }
          ],
          "rowLimit": 500,
          "filter": true
        }
      },
      "name": "LogsData"
    }
